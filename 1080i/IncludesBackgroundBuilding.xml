<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="CommonBackground">
		<control type="group">
			<animation effect="fade" time="600">WindowOpen</animation>
			<animation effect="fade" time="600">WindowClose</animation>
			<include condition="Window.IsActive(home)">HomeFanart</include>
			<include condition="!Window.IsActive(home)">GlobalFanart</include>
			<include condition="Window.IsActive(home)">LiveCasesBackground</include>
			<include condition="Window.IsActive(videos) + !IsEmpty(Skin.String(VideoGenreFanartPath))">VideoGenreFanart</include>
			<include condition="Window.IsActive(musiclibrary) + !IsEmpty(Skin.String(MusicGenreFanartPath))">MusicGenreFanart</include>
			<include condition="Window.IsActive(script-NextAired-TVGuide.xml) | Window.IsActive(script-NextAired-TVGuide2.xml)">TVShowNextAiredBackground</include>
			<include condition="Window.IsActive(weather)">WeatherBackground</include>
			<include condition="Window.IsActive(script-globalsearch-main.xml)">GlobalSearchBG</include>
			<control type="visualisation">
				<include>FullscreenDimensions</include>
				<!-- <include>AnimatedBackgroundAnimation</include> -->
				<visible>Player.HasAudio + !Skin.HasSetting(hidevizbehindfanart)</visible>
				<visible>!Skin.HasSetting(Disable.ShowBackgroundVis)</visible>
				<visible>!Window.IsVisible(TVChannels) + !Window.IsVisible(RadioChannels)</visible>
				<visible>IsEmpty(Window(Videos).Property(PlayingBackgroundMedia))</visible>
			</control>
		</control>
		<control type="group">
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture background="true">black.png</texture>
				<visible>[Player.HasVideo + !Skin.HasSetting(Disable.ShowBackgroundVideo) + !Window.IsVisible(TVChannels) + !Window.IsVisible(RadioChannels)]</visible>
			</control>
			<control type="videowindow">
				<include>FullscreenDimensions</include>
				<visible>Player.HasVideo + !Skin.HasSetting(Disable.ShowBackgroundVideo) + !Window.IsVisible(TVChannels) + !Window.IsVisible(RadioChannels)</visible>
			</control>
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture background="true">black.png</texture>
				<visible>!Window.IsVisible(skinsettings)</visible>
				<visible>Control.IsVisible(502) | Control.IsVisible(503) | Control.IsVisible(504) | Control.IsVisible(505) | Control.IsVisible(508) | Control.IsVisible(609) | [Control.IsVisible(507) + Skin.HasSetting(alternatebiglistlayout)] | [Control.IsVisible(51) + Skin.HasSetting(Enable.AlternateInfowall)] | [Player.HasVideo + !Skin.HasSetting(Disable.ShowBackgroundVideo) + Control.IsVisible(701)] | Control.IsVisible(700)</visible>
				<animation effect="fade" start="100" end="55" time="240" condition="true">conditional</animation>
			</control>
			<control type="group">
				<visible>Skin.HasSetting(MakeItSnow) + !Player.HasVideo</visible>
				<control type="image">
					<include>FullscreenDimensions</include>
					<texture background="true">black.png</texture>
					<animation effect="fade" start="100" end="20" time="240" condition="true">conditional</animation>
					<visible>![Control.IsVisible(502) | Control.IsVisible(503) | Control.IsVisible(504) | Control.IsVisible(505) | Control.IsVisible(508) | Control.IsVisible(609) | [Control.IsVisible(507) + Skin.HasSetting(alternatebiglistlayout)] | [Control.IsVisible(51) + Skin.HasSetting(Enable.AlternateInfowall)] | Control.IsVisible(700)]</visible>
				</control>
				<include condition="Skin.HasSetting(MakeItSnow)">SnowBackground</include>
			</control>
		</control>
		<control type="group">
			<visible>!Window.IsActive(skinsettings)</visible>
			<visible>Control.IsVisible(50) | Control.IsVisible(602) | Control.IsVisible(55) | Control.IsVisible(500) | Control.IsVisible(499) | [Control.IsVisible(51) + !Skin.HasSetting(Enable.AlternateInfowall)] | Control.IsVisible(58) | Control.IsVisible(601) | Control.IsVisible(52) | Control.IsVisible(53) | [Control.IsVisible(507) + !Skin.HasSetting(alternatebiglistlayout)] | Control.IsVisible(54)</visible>
			<control type="image">
				<left>0</left>
				<top>0</top>
				<width>696</width>
				<height>1080</height>
				<texture border="1">vignette.png</texture>
			</control>
			<control type="image">
				<left>696r</left>
				<top>0</top>
				<width>696</width>
				<height>1080</height>
				<texture flipx="true" border="1">vignette.png</texture>
			</control>
		</control>
		<control type="group">
			<include>Window_OpenClose_Animation</include>
			<visible>Control.IsVisible(50) | [Control.IsVisible(51) + !Skin.HasSetting(Enable.AlternateInfowall)] | Control.IsVisible(52) | Control.IsVisible(53) | Control.IsVisible(54) | Control.IsVisible(55) | Control.IsVisible(56) | Control.IsVisible(57) | Control.IsVisible(58) | Control.IsVisible(499) | Control.IsVisible(500) | Control.IsVisible(601) | Control.IsVisible(602) | Control.IsVisible(701) | [Control.IsVisible(507) + !Skin.HasSetting(alternatebiglistlayout)] | Window.IsActive(script-NextAired-TVGuide.xml) | Window.IsActive(script-NextAired-TVGuide2.xml) | Window.IsActive(SettingsCategory.xml) | Window.IsActive(script-globalsearch-main.xml) | Window.IsActive(weather) | [Control.IsVisible(700) + Container.Content(movies) + IsEmpty(Skin.String(700.movies.ViewFloorStatus))] | [Control.IsVisible(700) + Container.Content(tvshows) + IsEmpty(Skin.String(700.tvshows.ViewFloorStatus))] | [Control.IsVisible(700) + Container.Content(seasons) + IsEmpty(Skin.String(700.seasons.ViewFloorStatus))] | [Control.IsVisible(700) + Container.Content(episodes) + IsEmpty(Skin.String(700.episodes.ViewFloorStatus))] | Window.IsActive(script-globalsearch-main.xml) | Window.IsActive(skinsettings)</visible>
			<include>ViewAnimation</include>
			<animation effect="slide" start="0,0" end="0,80" time="720" tween="cubic" easing="inout" condition="![[Control.IsVisible(52) + !Window.IsActive(systeminfo)] | Control.IsVisible(53) | Control.IsVisible(54) | Control.IsVisible(56) | Control.IsVisible(500) | Control.IsVisible(57)] | Window.IsActive(skinsettings) | Window.IsActive(1140)">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,40" time="720" tween="cubic" easing="inout" condition="Control.IsVisible(700) + !Window.IsActive(skinsettings)">Conditional</animation>
			<animation effect="slide" end="0,70" time="0" reversible="false" condition="Control.IsVisible(53) + Container.Content(genres)">Conditional</animation>
			<control type="image">
				<left>0</left>
				<top>400</top>
				<width>FullScreenWidth</width>
				<height>680</height>
				<texture>floor.png</texture>
			</control>
		</control>
		<control type="group">
			<include>Window_OpenClose_Animation</include>
